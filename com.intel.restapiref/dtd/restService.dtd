<!ENTITY % pre          "pre     | %pr-d-pre;" >
<!ENTITY % keyword      "keyword | %pr-d-keyword;  | %http-d-keyword;" >
<!ENTITY % ph           "ph      | %pr-d-ph;       | %hi-d-ph;   |  %http-d-ph;"  >
